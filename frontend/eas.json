{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "image": "latest",
        "env": {
          "KOTLIN_VERSION": "1.9.25"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
